              </tr>
            </thead>
            <tbody>
              <% var i = 1; %> 
                <%users.forEach((item)=>{%>   
              <tr>
                <th class="row"><%= i; %> <%! i++; %></th>
               
                <td><%=item.name %></td>
                <td><%=item.email %></td>
                <td><%=item.balance %></td>
                <td><a href="/transactionHistory/<%=item.id%>">transfer</a></td>
                 
              </tr>
              <%});%>

              </tbody>
        </table>
